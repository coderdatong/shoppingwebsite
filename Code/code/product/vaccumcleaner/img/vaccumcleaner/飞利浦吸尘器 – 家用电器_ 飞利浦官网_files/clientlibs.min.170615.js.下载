d.activeTab=c.activeTab
};
philips.components.aggregated.p54v3MediaGallery.Video.prototype={initiateVideoPlayer:function(){var c=this,d=["SC7","PM1","PM2","PM3","PM4","PM5","HQ1","HQ2","HQ3","HQ4","HQ5","CLV","FML","FEV"];
c.activeItem=c.activeTab.find("li[data-asset-index="+c.itemIndex+"]");
c.videoType=c.activeItem.attr("data-video-type");
c.videoID=c.activeItem.attr("data-video-id");
if(c.videoType==="youtube"){c.component="sc01v2YouTubeWidget";
c.timeInterval=setInterval(c.initiatePlayer.bind(c),1000)
}else{if(c.videoType==="youku"){c.component="sc11YoukuWidget";
c.timeInterval=setInterval(c.initiatePlayer.bind(c),1000)
}else{if($.inArray(c.videoType,d)!==-1){c.videoType="scene7";
c.component="sc21Scene7Video";
c.timeInterval=setInterval(c.initiatePlayer.bind(c),1000)
}}}},initiatePlayer:function(){var d=this;
var c=Handlebars.compile($("#p-p54v3-"+d.videoType+"-video-template").html())({videoid:d.videoID})||"";
if(c!==""){d.activeItem.find(".p-large-thumbnail .p-video-player").html(c);
d.videoSection=d.activeItem.find(".p-video-player .p-player");
philips.components.aggregated[d.component].create(d.videoSection);
if(d.videoSection){clearInterval(d.timeInterval)
}}},playVideo:function(){var b=this;
if(b.videoSection){b.videoSection.find("."+b.videoSection.attr("data-video-link")).trigger("click")
}},closeVideoPlayer:function(){var b=this;
b.activeItem.find(".p-sc01v2-youtube-widget").each(function(){if(typeof $(this).data("sc01v2YouTubeWidget")!=="undefined"){$(this).data("sc01v2YouTubeWidget").closeVideo()
}});
b.activeItem.find(".p-video-player").find("section").remove()
}};
philips.components.aggregated.p54v3MediaGallery.Carousel=function(c){var d=this;
d.activeTab=c.activeTab;
d.modeView=c.modeView
};
philips.components.aggregated.p54v3MediaGallery.Carousel.prototype={init:function(){var b=this;
b.viewerWrapper=b.activeTab.find(".p-carousel-wrapper."+b.modeView);
b.viewerCarouselItems=b.viewerWrapper.find(".p-carousel-view li");
b.totalPages=b.viewerCarouselItems.length;
b.pager=b.activeTab.find("."+b.modeView+" .p-carousel-indicator");
b.navigator=b.activeTab.find(".p-carousel-controls."+b.modeView);
b.viewerCarouselIndicator=b.activeTab.find("."+b.modeView+" .p-carousel-indicator");
b.currentPage=0;
b.generatePager();
b.viewerCarousel=philips.libs.carousel.instantiate(b.viewerWrapper.get(0));
if(typeof(window.Hammer)!=="undefined"){b.viewerWrapper.hammer({drag_max_touches:0}).on("swipeleft",function(){b.updatePagerIndex.call(b,b.getIndex.call(b,true))
}).on("swiperight",function(){b.updatePagerIndex.call(b,b.getIndex.call(b,false))
})
}b.viewerCarouselIndicator.find("li").on("click",function(){b.handlePagerClick($(this).attr("data-index"))
});
b.activeTab.find("."+b.modeView+" .p-nav-prev").on("click",function(a){a.preventDefault();
b.prev()
});
b.activeTab.find("."+b.modeView+" .p-nav-next").on("click",function(a){a.preventDefault();
b.next()
})
},next:function(){var b=this;
b.animateTo(b.getIndex(true));
philips.components.aggregated.p54v3MediaGallery.tracking.trackInteraction("navigation_arrow_right")
},prev:function(){var b=this;
b.animateTo(b.getIndex(false));
philips.components.aggregated.p54v3MediaGallery.tracking.trackInteraction("navigation_arrow_left")
},getIndex:function(e){var d=this,f;
if(e){if(philips.isRTL){f=d.currentPage<=0?d.totalPages-1:d.currentPage-1
}else{f=d.currentPage>=(d.totalPages-1)?0:d.currentPage+1
}}else{if(philips.isRTL){f=d.currentPage>=(d.totalPages-1)?0:d.currentPage+1
}else{f=d.currentPage<=0?d.totalPages-1:d.currentPage-1
}}return f
},generatePager:function(){var d=this,e=[],f;
if(d.pager&&d.totalPages>1){d.pager.empty();
for(f=0;
f<d.totalPages;
f++){e[f]=document.createElement("li");
if(f===0){$(e[f]).attr("class","p-active")
}$(e[f]).attr("data-index",f)
}d.pager.html(e)
}if(d.modeView!=="p-detail-view"&&!d.pager.find("li").length){d.navigator.hide()
}},handlePagerClick:function(c){var d=this;
d.animateTo(c)
},animateTo:function(d){var e=this,f;
f=parseInt(d,10);
e.viewerCarousel.setIndex(f);
e.updatePagerIndex(f)
},updatePagerIndex:function(c){var d=this;
$(d.pager.find("li")).removeClass("p-active");
$(d.pager.find("li")[c]).addClass("p-active");
if(d.currentPage===c){d.viewerWrapper.css("visibility","visible")
}else{d.currentPage=c
}},reRenderCarousel:function(){var b=this;
b.viewerCarousel.handleResize()
}};
philips.components.aggregated.p54v3MediaGallery.DetailView=function(c){var d=this;
d.category=c.category;
d.itemIndex=c.index;
d.activeTab=c.activeTab;
d.modeView=c.modeView;
d.data=c.data
};
philips.components.aggregated.p54v3MediaGallery.DetailView.prototype={init:function(){var d=this,c={modeView:d.modeView,itemIndex:d.itemIndex,activeTab:d.activeTab};
d.carInstance=new philips.components.aggregated.p54v3MediaGallery.Carousel(c);
d.carInstance.init();
d.activeTab.find(".p-detail-view .p-show-less").on("click",function(a){a.preventDefault();
d.enableTrunk8()
});
d.activeTab.find(".p-detail-view .p-description").on("click",function(a){a.preventDefault();
d.toggleTrunk8()
});
d.activeTab.find(".p-detail-view .p-show-more").on("click",function(a){a.preventDefault();
d.disableTrunk8()
});
d.activeTab.find(".p-detail-view .p-play-video, .p-detail-view .p-product-image").on("click",function(a){a.preventDefault();
if(d.videoInstance){d.videoInstance.playVideo()
}});
d.carInstance.viewerCarousel._$outerWrapper.on("indexChange",function(b,f,a){d.itemIndex=f;
d.pastIndex=a;
d.activateDetailViewFunc()
});
d.carInstance.viewerCarousel._$outerWrapper.on("indexChanged",function(a,b){d.removeZoomContainer();
d.carInstance.viewerWrapper.css("visibility","visible")
})
},activateDetailViewFunc:function(c){var d=this;
d.itemIndex=c?parseInt(c,10):d.itemIndex;
d.itemData=d.data[d.itemIndex];
d.activeItem=d.activeTab.find('.p-detail-view li[data-asset-index="'+d.itemIndex+'"]');
d.s7ID=d.itemData.image.split(philips.scene7.imgRoot)[1];
if(typeof d.pastIndex!=="undefined"){d.pastItem=d.activeTab.find('.p-detail-view li[data-asset-index="'+d.pastIndex+'"]')
}switch(d.category){case"VID":d.closeActivePlayer();
d.initiateVideoPlayer();
break;
case"CLI":case"PI":d.activeTab.find(".p-collection-view .p-gallery-item").removeClass("p-current");
d.activeTab.find(".p-collection-view .p-gallery-item[data-asset-index="+d.itemIndex+"]").addClass("p-current");
if(d.s7ID){d.initiateBasicZoomViewer()
}break;
default:break
}d.$overviewContent=d.activeItem.find(".p-description");
if(d.$overviewContent.outerHeight()>86){d.$overviewContent.addClass("p-trunk8-true");
d.enableTrunk8()
}else{if(d.$overviewContent.hasClass("p-trunk8-true")){d.activeItem.find(".p-show-more").show()
}else{d.activeItem.find(".p-show-more").hide()
}}},initiateVideoPlayer:function(){var d=this;
