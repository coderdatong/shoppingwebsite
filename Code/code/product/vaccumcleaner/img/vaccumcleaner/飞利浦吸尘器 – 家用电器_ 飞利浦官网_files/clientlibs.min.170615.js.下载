var l={defaults:{duration:0,offset:0,triggerElement:undefined,triggerHook:0.5,reverse:true,loglevel:2},validate:{offset:function(a){a=parseFloat(a);
if(!h.type.Number(a)){throw ['Invalid value for option "offset":',a]
}return a
},triggerElement:function(a){a=a||undefined;
if(a){var b=h.get.elements(a)[0];
if(b){a=b
}else{throw ['Element defined in option "triggerElement" was not found:',a]
}}return a
},triggerHook:function(a){var b={onCenter:0.5,onEnter:1,onLeave:0};
if(h.type.Number(a)){a=Math.max(0,Math.min(parseFloat(a),1))
}else{if(a in b){a=b[a]
}else{throw ['Invalid value for option "triggerHook": ',a]
}}return a
},reverse:function(a){return !!a
},loglevel:function(a){a=parseInt(a);
if(!h.type.Number(a)||a<0||a>3){throw ['Invalid value for option "loglevel":',a]
}return a
}},shifts:["duration","offset","triggerHook"]};
