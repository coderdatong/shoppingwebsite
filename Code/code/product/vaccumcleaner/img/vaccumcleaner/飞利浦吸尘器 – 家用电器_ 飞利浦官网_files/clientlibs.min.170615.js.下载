}},error:function(){K(a())
}})
}function K(o){if(o){B.removeClass("p-hidden");
M=M?M:(I["360"]?"360":Object.keys(I)[0]);
if(Object.keys(I).length!==1){N()
}else{B.attr("data-current-tab",M)
}g.on(l.MODE_CHANGE,C);
g.on(g.RESIZE,F);
if(I.VID){O(I.VID)
}D=new philips.components.aggregated.p54v3MediaGallery.Tabs({component:B,galleryAssets:I,activeCAT:M,activeView:P});
D.init()
}else{B.addClass("p-hidden")
}}function N(){var q={"360":"360",CLI:"Clinical",PI:"Product",VID:"Videos"},r=[],o="collection-view";
var p=Handlebars.compile(B.find("#p-gallery-nav-tabs-template").html())||"";
if(p!==""){m.each(Object.keys(I),function(u,t){o=t==="360"?"360-view":"collection-view";
r[u]={key:t,value:q[t],view:o}
});
J.append(p({activeCAT:M,tabsData:r}));
philips.components.aggregated.navTabs.create(B.find(".p-nav-tabs"))
}}function O(o){m.each(o,function(q,p){if(p.videoType==="youku"){m.get("https://openapi.youku.com/v2/videos/show.json?video_id="+p.videoId+"&client_id=14c02d328f920adb",function(t){var r=m.map(o,function(u,v){if(u.videoId===t.id){return v
}});
m.each(r,function(v,u){o[u].thumbnail=t.thumbnail;
o[u].image=t.bigThumbnail
})
})
}})
}function C(p,o,q){D.handleModeChange(p,o,q)
}function F(){D.handleResize()
}B.data("p54v3MediaGallery",c);
k.push(c);
return c
}return{toString:function(){return"[object P_54v3 Media Gallery]"
},instances:k,create:h}
})(jQuery);
philips.components.aggregated.p54v3MediaGallery.tracking=(function(c){var d={};
d.trackInteraction=function(a){if(philips.analytics){philips.analytics.trackConversion({name:"interaction",description:a,compId:"p54v3"})
}};
return d
})(jQuery);
philips.components.aggregated.p54v3MediaGallery.Tabs=function(c){var d=this;
d.component=c.component;
d.navTab=c.component.find(".p-nav-tab-categories");
d.navTabs=c.component.find(".p-nav-tabs");
d.navTabItems=c.component.find(".p-nav-tab-item");
d.tabContainer=c.component.find(".p-tab-container");
d.galleryData=c.galleryAssets;
d.activeCategory=c.activeCAT;
d.activeView=c.activeView;
d.singleProdImage=d.component.attr("data-single-product-image")==="true"?true:false;
d.PICount=d.galleryData.PI?d.galleryData.PI.length:0;
d.CLICount=d.galleryData.CLI?d.galleryData.CLI.length:0;
d.VIDCount=d.galleryData.VID?d.galleryData.VID.length:0;
d.itemsPerTab={PI:d.PICount,CLI:d.CLICount,VID:d.VIDCount};
d.activeTab=d.tabContainer.find(".p-tab[data-tab-category="+d.activeCategory+"]");
d.activeItemIndex=0;
d.currentMode=philips.responsive.getMode();
d.readMoreText=d.component.attr("data-read-more");
d.readLessText=d.component.attr("data-read-less");
d.collapse=d.component.find(".p-collapse");
d.collapseButton=d.collapse.find("button");
d.collapseUl=d.collapse.find("ul")
};
philips.components.aggregated.p54v3MediaGallery.Tabs.prototype={init:function(){var f=this,h=f.component.find(".p-clinical-filter"),e;
f.appliedSingleProductRule={PI:false,CLI:false,VID:false};
f.galleryModulesInst={};
f.categoryMenu=f.component.find(".p-filter-wrapper");
f.filterCategory="";
f.filteredData={};
var g=[];
g[0]=$("<option/>",{value:"",html:"All Images"});
f.galleryAssets={};
f.galleryAssets["360"]=f.galleryData["360"];
f.galleryAssets.PI=f.galleryData.PI;
f.galleryAssets.VID=f.galleryData.VID;
f.openTab();
if(f.galleryData.CLI){h.find("option").each(function(a){e=this.value;
if(a){f.filteredData[e]=$.grep(f.galleryData.CLI,function(b){if(b.subject&&b.subject.indexOf(e)!==-1){return b
}});
$(this).html(this.innerHTML+" ("+f.filteredData[e].length+")")
}});
h.on("change",function(){f.applyFilter(this)
})
}f.navTabItems.on("click",function(a){if(!$(this).hasClass("p-active")){f.tabClickEventHandler(a)
}})
},openTab:function(f){var e=this;
e.component.removeClass("p-clinical-active");
if(e.activeCategory==="CLI"){e.component.addClass("p-clinical-active")
}if(e.filterCategory!==""&&e.galleryData.CLI){e.galleryAssets.CLI=e.filteredData[e.filterCategory]
}else{e.galleryAssets.CLI=e.galleryData.CLI
}e.showActiveTab();
e.updateNavTabs();
if(e.activeCategory==="360"&&!e.galleryModulesInst.pex360Instance){var d={activeTab:e.activeTab};
e.activeTab.find(".p-inner-container").addClass("p-active");
e.galleryModulesInst.pex360Instance=new philips.components.aggregated.p54v3MediaGallery.Pex360(d);
e.galleryModulesInst.pex360Instance.init()
}if(e.galleryAssets[e.activeCategory]){if(e.itemsPerTab[e.activeCategory]===1){if(!e.appliedSingleProductRule[e.activeCategory]&&!e.galleryModulesInst[e.activeCategory+"DetViewInstance"]){e.appliedSingleProductRule[e.activeCategory]=true;
e.applySingleProductRule(f)
}}else{e.renderListView();
e.initializeActiveModule();
e.showActiveTabContent()
}e.reRenderCarousel()
}},toggleFilter:function(){var b=this;
b.categoryMenu.find(".p-clinical-filter").removeAttr("disabled");
b.categoryMenu.find(".p-label").removeClass("p-disabled");
if(b.activeView==="detail-view"){b.categoryMenu.find(".p-clinical-filter").attr("disabled","disabled");
b.categoryMenu.find(".p-label").addClass("p-disabled")
}},applySingleProductRule:function(c){var d=this;
d.activeView="detail-view";
if(!d.activeTab.find(".p-inner-container").hasClass("p-"+d.activeView)){d.activeTab.find(".p-inner-container").addClass("p-detail-view");
d.renderDetailView();
d.activeTab.find(".p-view-close").addClass("p-hidden");
d.activeTab.find(".p-carousel-controls").css("display","none")
}if(d.galleryModulesInst[d.activeCategory+"DetViewInstance"]&&!c){d.galleryModulesInst[d.activeCategory+"DetViewInstance"].itemIndex=0;
d.galleryModulesInst[d.activeCategory+"DetViewInstance"].removeZoomContainer(true);
d.galleryModulesInst[d.activeCategory+"DetViewInstance"].activateDetailViewFunc()
}},showActiveTab:function(){var b=this;
b.navTabItems.removeClass("p-bold p-active");
b.navTab.find(".p-nav-tab-item[data-tab-category="+b.activeCategory+"]").addClass("p-bold p-active");
b.tabContainer.find(".p-tab").removeClass("p-active");
b.tabContainer.find(".p-tab[data-tab-category="+b.activeCategory+"]").addClass("p-active");
b.activeTab=b.tabContainer.find(".p-tab.p-active")
},showActiveTabContent:function(){var d=this,c;
d.activeTab.find(".p-inner-container").removeClass("p-active");
d.activeTab.find(".p-carousel-controls").removeClass("p-active");
d.activeTab.find(".p-nav-controls").removeClass("p-active");
switch(d.activeView){case"collection-view":if(d.currentMode<3){d.activeTab.find(".p-s-collection-view").addClass("p-active");
c=".p-s-collection-view"
}else{d.activeTab.find(".p-l-collection-view").addClass("p-active");
c=".p-l-collection-view"
}break;
case"detail-view":d.activeTab.find(".p-detail-view").addClass("p-active");
break;
default:break
}if(d.activeTab.find(".p-carousel-wrapper"+c+" li").length<=1){d.activeTab.find(".p-nav-controls"+c).removeClass("p-active")
}d.activeTab.attr("data-tab-asset-view",d.activeView);
d.navTab.find(".p-active").attr("data-tab-asset-view",d.activeView)
},tabClickEventHandler:function(f){var e=this;
f.preventDefault();
e.activeCategory=$(f.currentTarget).attr("data-tab-category");
e.activeView=$(f.currentTarget).attr("data-tab-asset-view");
e.collapse.toggleClass("p-closed");
e.collapse.toggleClass("p-open");
e.component.attr("data-current-tab",e.activeCategory);
e.openTab(true);
e.toggleFilter();
if(e.activeView==="detail-view"&&e.galleryModulesInst[e.activeCategory+"DetViewInstance"]){e.galleryModulesInst[e.activeCategory+"DetViewInstance"].removeZoomContainer(true);
e.galleryModulesInst[e.activeCategory+"DetViewInstance"].closeActivePlayer();
var d=e.activeTab.find(".p-current").length?e.activeTab.find(".p-current").attr("data-asset-index"):0;
e.galleryModulesInst[e.activeCategory+"DetViewInstance"].itemIndex=parseInt(d,10);
e.galleryModulesInst[e.activeCategory+"DetViewInstance"].activateDetailViewFunc()
}philips.components.aggregated.p54v3MediaGallery.tracking.trackInteraction("mediagallery_"+f.target.text)
},galItemClickHandler:function(g,e){var f=this,h;
g.find("."+e+" .p-gallery-item").on("click",function(a){a.preventDefault();
g.find("."+e+" .p-gallery-item").removeClass("p-current");
$(a.currentTarget).addClass("p-current");
f.activeItemIndex=$(a.currentTarget).attr("data-asset-index");
f.activeView="detail-view";
if(!f.activeTab.find(".p-inner-container").hasClass("p-"+f.activeView)){f.renderDetailView()
}f.activeTab.find(".p-carousel-wrapper.p-detail-view").css("visibility","hidden");
f.toggleFilter();
if(parseInt(f.activeItemIndex,10)===f.galleryModulesInst[f.activeCategory+"DetViewInstance"].itemIndex){f.galleryModulesInst[f.activeCategory+"DetViewInstance"].activateDetailViewFunc();
f.galleryModulesInst[f.activeCategory+"DetViewInstance"].carInstance.viewerWrapper.css("visibility","visible");
f.galleryModulesInst[f.activeCategory+"DetViewInstance"].removeZoomContainer()
}else{f.galleryModulesInst[f.activeCategory+"DetViewInstance"].itemIndex=parseInt(f.activeItemIndex,10);
f.galleryModulesInst[f.activeCategory+"DetViewInstance"].carInstance.reRenderCarousel();
f.galleryModulesInst[f.activeCategory+"DetViewInstance"].carInstance.animateTo(f.activeItemIndex)
}f.showActiveTabContent();
h=f.activeCategory!=="VID"?$(a.currentTarget).attr("data-image-asset").split("?")[0].split("PhilipsConsumer/")[1]:$(a.currentTarget).attr("data-video-id");
philips.components.aggregated.p54v3MediaGallery.tracking.trackInteraction("gallery_icon_"+h)
})
},renderListView:function(){var h=this,m=h.component.find(".p-tab[data-tab-category="+h.activeCategory+"]"),p=h.getActiveViewTemplate("collection-view"),n=h.galleryAssets[h.activeCategory],l,o,k=h.activeCategory==="CLI"?"000000":"ffffff";
if(h.currentMode<3&&!m.find(".p-s-collection-view").length){l={items:n,bgc:k,mode:8,"class":"p-s-collection-view"};
m.prepend(p(l));
h.galItemClickHandler(m,"p-s-collection-view");
h.modeView="p-s-collection-view"
}if(h.currentMode>=3&&!m.find(".p-l-collection-view").length){o=n.length>9?"p-m-one-quarter":(n.length<5?"p-m-one-half":"p-m-one-third");
l={items:n,bgc:k,mode:16,"class":"p-l-collection-view",layoutClass:o};
m.append(p(l));
h.galItemClickHandler(m,"p-l-collection-view");
h.modeView="p-l-collection-view"
}},renderDetailView:function(){var e=this,d=e.getActiveViewTemplate("detail-view");
var f={items:e.galleryAssets[e.activeCategory],pages:e.galleryAssets[e.activeCategory].length,readLess:e.readLessText,readMore:e.readMoreText};
e.activeTab.append(d(f));
e.modeView="p-detail-view";
e.initializeActiveModule();
e.showActiveTabContent();
e.activeTab.find(".p-view-close").on("click",function(a){a.preventDefault();
e.galleryModulesInst[e.activeCategory+"DetViewInstance"].removeZoomContainer(true);
e.galleryModulesInst[e.activeCategory+"DetViewInstance"].closeActivePlayer();
e.activeView="collection-view";
e.showActiveTabContent();
e.reRenderCarousel();
e.toggleFilter()
})
},initializeActiveModule:function(){var e=this;
var f=e.activeCategory;
var d={category:f,index:0,activeTab:e.activeTab,modeView:e.modeView,data:e.galleryAssets[f]};
switch(true){case (e.modeView==="p-l-collection-view"&&e.currentMode>=3&&!e.galleryModulesInst[f+"LColViewInstance"]):e.galleryModulesInst[f+"LColViewInstance"]=new philips.components.aggregated.p54v3MediaGallery.ListView(d);
e.galleryModulesInst[f+"LColViewInstance"].init();
break;
case (e.modeView==="p-s-collection-view"&&e.currentMode<3&&!e.galleryModulesInst[f+"SColViewInstance"]):e.galleryModulesInst[f+"SColViewInstance"]=new philips.components.aggregated.p54v3MediaGallery.ListView(d);
e.galleryModulesInst[f+"SColViewInstance"].init();
break;
case (e.activeView==="detail-view"&&!e.galleryModulesInst[f+"DetViewInstance"]):e.galleryModulesInst[f+"DetViewInstance"]=new philips.components.aggregated.p54v3MediaGallery.DetailView(d);
e.galleryModulesInst[f+"DetViewInstance"].init();
break;
default:break
}},getActiveViewTemplate:function(e){var d=this,f;
switch(e){case"collection-view":f=Handlebars.compile(d.component.find("#p-gallery-grid-view-template").html())||"";
break;
case"detail-view":f=Handlebars.compile(d.component.find("#p-gallery-detail-view-template").html())||"";
break;
default:break
}return f
},applyFilter:function(c){var d=this;
d.filterCategory=$(c).find("option:selected").get(0).value;
d.activeTab.find(".p-s-collection-view, .p-l-collection-view, .p-detail-view").each(function(){$(this).remove()
});
d.galleryModulesInst.CLILColViewInstance="";
d.galleryModulesInst.CLISColViewInstance="";
d.galleryModulesInst.CLIDetViewInstance="";
d.openTab()
},updateNavTabs:function(){var b=this;
if(b.currentMode<3){b.navTab.find(".p-nav-tab-item").each(function(a,d){if($(d).attr("data-tab-category")===b.activeCategory){$(d).parent().insertBefore(b.collapse)
}else{$(d).parent().appendTo(b.collapseUl)
}})
}b.navTab.removeClass("p-nocollapse");
if(b.navTab.find("li:not(.p-collapse)").length<=1){b.navTab.addClass("p-nocollapse")
}},reRenderCarousel:function(){var d=this,c=d.activeCategory;
if(d.activeView==="collection-view"){d.galleryModulesInst[c+"LColViewInstance"]?d.galleryModulesInst[c+"LColViewInstance"].reRenderCarousel():"";
d.galleryModulesInst[c+"SColViewInstance"]?d.galleryModulesInst[c+"SColViewInstance"].reRenderCarousel():""
}else{d.galleryModulesInst[c+"DetViewInstance"]?d.galleryModulesInst[c+"DetViewInstance"].reRenderCarousel():""
}d.setCollapseWidth()
},setCollapseWidth:function(){var b=this;
b.collapseUl.css("width","100%")
},handleModeChange:function(h,g,e){var f=this;
f.currentMode=e;
f.updateNavTabs();
f.openTab();
f.showActiveTabContent()
},handleResize:function(){var b=this;
b.reRenderCarousel()
}};
