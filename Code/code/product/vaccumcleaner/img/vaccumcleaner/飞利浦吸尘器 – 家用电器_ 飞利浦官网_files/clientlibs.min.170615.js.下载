}return c/2*((k-=2)*k*k*k*k+2)+b
},easeInSine:function(l,k,b,c,d){return -c*Math.cos(k/d*(Math.PI/2))+c+b
},easeOutSine:function(l,k,b,c,d){return c*Math.sin(k/d*(Math.PI/2))+b
