jQuery.extend(jQuery.easing,{def:"easeOutQuad",swing:function(l,k,b,c,d){return jQuery.easing[jQuery.easing.def](l,k,b,c,d)
},easeInQuad:function(l,k,b,c,d){return c*(k/=d)*k+b
},easeOutQuad:function(l,k,b,c,d){return -c*(k/=d)*(k-2)+b
},easeInOutQuad:function(l,k,b,c,d){if((k/=d/2)<1){return c/2*k*k+b
}return -c/2*((--k)*(k-2)-1)+b
},easeInCubic:function(l,k,b,c,d){return c*(k/=d)*k*k+b
