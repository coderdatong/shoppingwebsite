}return c/2*((k-=2)*k*k+2)+b
},easeInQuart:function(l,k,b,c,d){return c*(k/=d)*k*k*k+b
},easeOutQuart:function(l,k,b,c,d){return -c*((k=k/d-1)*k*k*k-1)+b
